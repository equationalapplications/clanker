{
  "name": "yours-brightly-ai",
  "version": "1.1.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "semantic-release": "semantic-release",
    "prepare": "husky install",
    "commit": "git-cz",
    "format": "prettier --write \"app/**/*.{js,jsx,json,md,ts,tsx}\"",
    "lint": "eslint . --fix --ext .js,.ts,.tsx && npm run format",
    "test": "jest",
    "test:watch": "jest --watch",
    "adb": "adb reverse tcp:8081 tcp:8081"
  },
  "dependencies": {
    "@react-native-firebase/analytics": "^16.5.0",
    "@react-native-firebase/app": "^16.5.0",
    "@react-native-firebase/auth": "^16.5.0",
    "@react-native-firebase/crashlytics": "^16.5.0",
    "@react-native-firebase/firestore": "^16.5.0",
    "@react-native-firebase/functions": "^16.5.0",
    "@react-native-firebase/perf": "^16.5.0",
    "dotenv": "^16.0.3",
    "expo": "~47.0.9",
    "expo-build-properties": "~0.4.1",
    "expo-constants": "~14.0.2",
    "expo-dev-client": "~2.0.1",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "~1.4.2",
    "react": "18.1.0",
    "react-native": "0.70.5",
    "react-native-paper": "^5.1.2",
    "react-native-safe-area-context": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.4.0",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "commitizen": "^4.2.6",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.31.0",
    "eslint-config-universe": "^11.1.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.1",
    "semantic-release": "^19.0.5",
    "semantic-release-react-native": "^1.8.0"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}